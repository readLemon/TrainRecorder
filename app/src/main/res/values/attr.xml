<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RichEditor">
        <attr name="image_height" format="integer"/>
        <attr name="image_space" format="integer"/>
        <attr name="text_init_hint" format="string"/>
        <attr name="text_size" format="integer"/>
        <attr name="text_color" format="color"/>
        <attr name="text_line_space" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="RichTextView">
        <attr name="image_height" format="integer"/>
        <attr name="image_bottom" format="integer"/>
        <attr name="text_view_hint" format="string"/>
        <attr name="text_size" format="dimension"/>
        <attr name="text_color" format="color"/>
        <attr name="text_line_space" format="dimension"/>
    </declare-styleable>
</resources>