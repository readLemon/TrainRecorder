<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RichEditor">
        <attr name="image_height" format="integer" />
        <attr name="image_space" format="integer" />
        <attr name="text_init_hint" format="string" />
        <attr name="text_size" format="integer" />
        <attr name="text_color" format="color" />
        <attr name="text_line_space" format="dimension" />
    </declare-styleable>

    <declare-styleable name="RichTextView">
        <attr name="rt_image_height" format="integer" />
        <attr name="rt_image_bottom" format="integer" />
        <attr name="rt_text_view_hint" format="string" />
        <attr name="rt_text_size" format="dimension" />
        <attr name="rt_text_color" format="color" />
        <attr name="rt_text_line_space" format="dimension" />
    </declare-styleable>

    <declare-styleable name="LineChartView">
        <attr name="line_chart_radius" format="integer" />
        <attr name="line_chart_top_left_radius" format="integer" />
        <attr name="line_chart_top_right_radius" format="integer" />
        <attr name="line_chart_bottom_right_radius" format="integer" />
        <attr name="line_chart_bottom_left_radius" format="integer" />
        <attr name="line_chart_bg_color" format="color" />
        <attr name="line_chart_bg_line_color" format="color" />
        <attr name="line_chart_data_line_color" format="color" />
    </declare-styleable>




</resources>